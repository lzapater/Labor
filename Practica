#include <iostream>

using namespace std;


class Estudiante
{
    private:
        string nombre;
        string apellido;
        string codigo;

    public:
        Estudiante(string n,string a,string c){
        n=nombre;
        a=apellido;
        c=codigo;
        }

    string dar_n(){return nombre;}
    string dar_a(){return apellido;}
    string dar_c(){return codigo;}


};

class Curso
{
    private:
        string nombre;
        string apellido;
        string codigo;
        string* alumnos;
        int cantidaAlumnos;

    public:
        Curso(string n,string apellido,string c,int cA){
        n=nombre;
        a=apellido;
        c=codigo;
        cA = cantidaAlumnos;

        }
    string dar_n(){return n;}
    string dar_a(){return a;}
    string dar_c(){return c;}

    string mod_n(string new_n){return new_n;}
    string mod_a(string new_a){return new_a;}
    string mod_c(string new_c){return new_c;}
    string mod_n(string new_n){return new_n;}
}


};


int main()
{
    Estudiante e1;
    e1("Luis","Zapater","1010");
    e1.dar_a();
}
